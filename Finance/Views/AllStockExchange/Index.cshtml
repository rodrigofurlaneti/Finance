@model List<Active>
@{

}
<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Imagem</th>
            <th scope="col">Tipo</th>
            <th scope="col">Código</th>
            <th scope="col">Nome</th>
            <th scope="col">Setor</th>
            <th scope="col">Preço</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row"><img style="height:20px;" src=@item.Logo.Small /></th>
                <td>@item.Kind</td>
                <td>@item.Symbol</td>
                <td>
                    <button type="button" class="btn btn-secondary" data-toggle="tooltip" data-placement="top" title="@item.CompanyName @item.Document">
                        @item.Name
                    </button>
                </td>
                <td>@item.Sector</td>
                <td>R$ @item.Price</td>
            </tr>
        }


    </tbody>
</table>